 FROM ventas.salidas_venta ia 
 inner join ventas.tabla_detalle td ON  td.cod_campo = ia.cod_tipo_salida_venta 
 INNER JOIN ventas.tabla t ON t.cod_tabla   = td.cod_tabla AND     t.nombre_tabla = 'TIPOS_SALIDA_VENTA' 
 inner join ventas.almacenes_venta a on a.cod_almacen_venta = ia.cod_almacen_venta
 inner join ventas.sucursal_ventas sv on sv.cod_sucursal = a.cod_sucursal_venta
 inner join public.empresa e on e.cod_empresa = sv.cod_empresa
 left outer join public.personal p on p.cod_personal = ia.cod_personal_autoriza
 where ia.cod_salida_venta = $P{cod_salida_venta}]]></queryString>

